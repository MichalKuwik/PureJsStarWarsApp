\tscale: 0,\r\n\t\t\t},\r\n\t\t\t0.2,\r\n\t\t)\r\n\t\t.staggerTo(\r\n\t\t\tships,\r\n\t\t\t1, {\r\n\t\t\t\ty: -30,\r\n\t\t\t\trepeat: -1,\r\n\t\t\t\tyoyo: true,\r\n\t\t\t},\r\n\t\t\t0.5,\r\n\t\t)\r\n\t\t.staggerTo(\r\n\t\t\tships,\r\n\t\t\t0.5, {\r\n\t\t\t\ty: 20,\r\n\t\t\t\tyoyo: true,\r\n\t\t\t},\r\n\t\t\t0.3,\r\n\t\t)\r\n\t\t.from(input, 0.5, {\r\n\t\t\twidth: 0,\r\n\t\t})\r\n\t\t.from(label, 0.2, {\r\n\t\t\ty: -20,\r\n\t\t\topacity: 0,\r\n\t\t})\r\n\t\t.set(btn, {\r\n\t\t\topacity: 1,\r\n\t\t})\r\n\t\t.from(select, 0.5, {\r\n\t\t\tdelay: 1,\r\n\t\t\tscaleY: 0,\r\n\t\t})\r\n\t\t.from(volume_wrapper,0.5,{opacity:0})\r\n}\r\n\r\nexport const hideErr = () => {\r\n\ttl1.to(robot, 0.3, {\r\n\t\tscaleY: 0,\r\n\t});\r\n};\r\n\r\nexport const showErr = () => {\r\n\ttl1.to(robot, 1.5, {\r\n\t\tscaleY: 1,\r\n\t\tease: 'elastic.out(1, 0.3)',\r\n\t});\r\n};\r\n\r\nexport const animSpeaker = (value) => {\r\n\ttl1.staggerFrom(lines_speaker,0.5,{opacity:0,repeat:'-1', yoyo: true},0.3);\r\n}\r\n\r\nexport default animation;"},"lineCount":null}},"error":null,"hash":"6db77be7fdfb8bfb97b2eefba6c5ea85","cacheData":{"env":{}}}